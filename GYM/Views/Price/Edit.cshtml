@model GYM.Models.Price

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<div class="content-wrapper">
    <!-- Container-fluid starts -->
    <div class="container-fluid">
        <!-- Main content starts -->
        <div>
            <!-- Row Starts -->
            <div class="row">
                <div class="col-sm-12 p-0">
                    <div class="main-header">
                        <h4>Edit Fitness Price</h4>
                        <ol class="breadcrumb breadcrumb-title breadcrumb-arrow">
                            <li class="breadcrumb-item">
                                <a href="#"><i class="icofont icofont-home"></i></a>
                            </li>
                            <li class="breadcrumb-item">
                                <a href="#">Price View</a>
                            </li>

                        </ol>
                    </div>
                </div>
            </div>
            <!-- Row end -->
            <!-- Row start -->
            <div class="row">
                <!-- Form Control starts -->
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <h5 class="card-header-text">Edit Price</h5>
                            <div class="f-right">
                                <a href="" data-toggle="modal" data-target="#input-type-Modal"><i class="icofont icofont-code-alt"></i></a>
                            </div>
                        </div>

                        <div class="card-block">
                            <form asp-action="Edit">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="Id" />
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Heading" class="control-label"></label>
                                        <input asp-for="Heading" class="form-control" />
                                        <span asp-validation-for="Heading" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Mony" class="control-label"></label>
                                        <input asp-for="Mony" class="form-control" />
                                        <span asp-validation-for="Mony" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="TrainerNumber" class="control-label"></label>
                                        <input asp-for="TrainerNumber" class="form-control" />
                                        <span asp-validation-for="TrainerNumber" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Services" class="control-label"></label>
                                        <input asp-for="Services" class="form-control" />
                                        <span asp-validation-for="Services" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Courses" class="control-label"></label>
                                        <input asp-for="Courses" class="form-control" />
                                        <span asp-validation-for="Courses" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Massages" class="control-label"></label>
                                        <input asp-for="Massages" class="form-control" />
                                        <span asp-validation-for="Massages" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Aerobics" class="control-label"></label>
                                        <input asp-for="Aerobics" class="form-control" />
                                        <span asp-validation-for="Aerobics" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="CreateDate" class="control-label"></label>
                                        <input asp-for="CreateDate" class="form-control" />
                                        <span asp-validation-for="CreateDate" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
            <div class="form-group">
                <label asp-for="ModifiedDate" class="control-label"></label>
                <input asp-for="ModifiedDate" class="form-control" />
                <span asp-validation-for="ModifiedDate" class="text-danger"></span>
            </div>
        </div>
                                <div class="col-md-2">
            <div class="form-group form-check" style="margin-top:28px;">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsActive" />&nbsp;&nbsp;&nbsp; @Html.DisplayNameFor(model => model.IsActive)
                </label>
            </div>
        </div>
                                <div class="col-md-2">
                                    <label class="control-label"></label>
                                    <div class="form-group">
                                        <input type="submit" value="Save" class="btn btn-primary" />
                                    </div>

                                </div>
                            </form>

                        </div>

                    </div>
                    <div>
                        <a asp-action="Index">Back to List</a>
                    </div>


                </div>
                <!-- Form Control ends -->

            </div>
            <!-- Row end -->

        </div>
        <!-- Main content ends -->
    </div>
    <!-- Container-fluid ends -->
</div>


